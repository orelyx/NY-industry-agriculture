@article{phamAviationEmissionInventory2010,
  title = {Aviation Emission Inventory Development and Analysis},
  author = {Pham, Viet Van and Tang, Jiangjun and Alam, Sameer and Lokan, Chris and Abbass, Hussein A.},
  date = {2010-12-01},
  journaltitle = {Environmental Modelling \& Software},
