@article{agrawalCharacterizationChemicalParticulate2008,
  title = {Characterization of Chemical and Particulate Emissions from Aircraft Engines},
  author = {Agrawal, Harshit and Sawant, Aniket A. and Jansen, Karel and Wayne Miller, J. and Cocker, David R.},
  date = {2008-06-01},
  journaltitle = {Atmospheric Environment},
  shortjournal = {Atmospheric Environment},
  volume = {42},
  number = {18},
  pages = {4380--4392},
  issn = {1352-2310},
  doi = {10.1016/j.atmosenv.2008.01.069},
  url = {https://www.sciencedirect.com/science/article/pii/S1352231008000319},
  urldate = {2021-03-20},
  abstract = {This paper presents a series of measurements from four on-wing, commercial aircraft engines, including two newer CFM56-7 engines and two earlier CFM56-3 engines. Samples were collected from each engine using a probe positioned behind the exhaust nozzle of the aircraft, chocked on a concrete testing pad. The emission factors for particulate matter mass, elemental and organic carbon, carbonyls, polycyclic aromatic hydrocarbons, n-alkanes, dioxins, metals and ions are reported for four different engine power setting modes. The emissions indices of particulate matter, elemental and organic carbon are highly power dependent for these engines. Particulate matter emission indices (gkg−1 fuel) are found to increase from 1.1E−02 to 2.05E−01 with increase in power from idle to 85\%. The elemental carbon to organic carbon varies from 0.5 to 3.8 with change in power from idle to 85\%. The carbonyl emissions are dominated by formaldehyde. The emission index of formaldehyde ranges from 2.3E−01 to 4.8E−01gkg−1 fuel. The distribution of metals depends on the difference in the various engines. The dioxin emissions from the aircraft engines are observed to be below detection limit.},
  langid = {english},
  keywords = {Aircraft,Carbonyls,Jet turbine emissions,PAHs,Particulate matter},
  file = {/home/eric/Zotero/storage/R8ALXTFZ/Agrawal et al. - 2008 - Characterization of chemical and particulate emiss.pdf;/home/eric/Zotero/storage/SLJGTPZ4/S1352231008000319.html}
}

